<p id="notice"><%= notice %></p>

<h1>Collect Lines</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @collect_lines.each do |collect_line| %>
      <tr>
        <td><%= link_to 'Show', collect_line %></td>
        <td><%= link_to 'Edit', edit_collect_line_path(collect_line) %></td>
        <td><%= link_to 'Destroy', collect_line, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Collect Line', new_collect_line_path %>
